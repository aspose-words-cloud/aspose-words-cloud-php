{
    "description": 	"Aspose Words Cloud SDK for PHP lets convert, process and generate reports in a variety (20+) of file formats in Aspose Cloud quickly and easily, with zero initial costs.",
    "name": 		"aspose/words-sdk-php",
	"license": "MIT",
    "type": 		"library",
    "keywords" : 	["aspose", "report", "convert", "doc", "docx", "rtf", "mail merge", "reporting", "pdf"],
    "homepage": "https://github.com/aspose-words-cloud/aspose-words-cloud-php",
    "authors": [
        {
            "name": "YaroslawEkimov",
            "homepage": "https://github.com/aspose-words-cloud/aspose-words-cloud-php"
        },
		{
            "name": "IvanovJohnAspose",
			"homepage": "https://github.com/aspose-words-cloud/aspose-words-cloud-php"
		}
    ],
	"config": {
        "platform": {
            "php": "5.6.32"
        }
    },
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
		"aspose/storage-sdk-php": "1.0.*",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^4.8"
    },
	"autoload": {
        "psr-0": {
            "Aspose": "src/",
            "BaseTest": "tests/"
        }
    }
}
